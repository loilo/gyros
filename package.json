{
  "name": "gyros",
  "version": "1.0.6",
  "type": "module",
  "description": "Transform PHP ASTs the easy way",
  "keywords": [
    "ast",
    "yufka",
    "falafel",
    "burrito",
    "source",
    "syntax",
    "traversal",
    "tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loilo/gyros.git"
  },
  "license": "MIT",
  "author": {
    "name": "Florian Reuschel",
    "email": "florian@loilo.de",
    "url": "https://loilo.de"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/gyros.js",
  "module": "./dist/gyros.js",
  "exports": {
    "import": "./dist/gyros.js"
  },
  "sideEffects": false,
  "scripts": {
    "test:lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:unit": "vitest run",
    "test": "run-s test:lint test:unit",
    "dev:vitest": "vitest",
    "dev": "vite build --watch",
    "build": "vite build && tsc",
    "prepublish": "run-s build"
  },
  "devDependencies": {
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "vite": "^4.0.4",
    "vitest": "^0.31.0"
  },
  "dependencies": {
    "magic-string": "^0.30.0",
    "php-parser": "^3.1.4"
  }
}
