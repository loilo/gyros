{
  "name": "gyros",
  "version": "1.0.6",
  "type": "module",
  "description": "Transform PHP ASTs the easy way",
  "keywords": [
    "ast",
    "yufka",
    "falafel",
    "burrito",
    "source",
    "syntax",
    "traversal",
    "tree"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/loilo/gyros.git"
  },
  "license": "MIT",
  "author": {
    "name": "Florian Reuschel",
    "email": "florian@loilo.de",
    "url": "https://loilo.de"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/gyros.js",
  "module": "./dist/gyros.js",
  "exports": {
    "import": "./dist/gyros.js"
  },
  "sideEffects": false,
  "scripts": {
    "test:lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:unit": "vitest run",
    "test": "run-s test:lint test:unit",
    "dev:vitest": "vitest",
    "dev": "vite build --watch",
    "build": "vite build && tsc",
    "prepublish": "run-s build"
  },
  "devDependencies": {
    "@types/node": "^20.4.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "typescript": "^5.0.4",
    "vite": "^4.4.2",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "magic-string": "^0.30.1",
    "php-parser": "^3.1.5"
  }
}
